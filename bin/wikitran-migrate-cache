#!/usr/bin/env php

<?php

use Wikitran\Core\Migration;

$autoloadExplicit = __DIR__ . '/../vendor/autoload.php';
$autoloadDependency = __DIR__ . '/../../../autoload.php';

if (file_exists($autoloadExplicit)) {
    require $autoloadExplicit;
} elseif (file_exists($autoloadDependency)) {
    require $autoloadDependency;
} else {
    exit('Autoload script not found.');
}

Migration::run();
